@model IEnumerable<MoneyMe.Web.Models.Scaffolding.AggregateRootEvent>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th></th>
        <th>
            AggregateId
        </th>
        <th>
            Version
        </th>
        <th>
            Data
        </th>
        <th>
            AggregateRoot
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
        <td>
			@item.AggregateId
        </td>
        <td>
			@item.Version
        </td>
        <td>
			@item.Data
        </td>
        <td>
			@Html.DisplayTextFor(_ => item.AggregateRoot).ToString()
        </td>
    </tr>
}

</table>


