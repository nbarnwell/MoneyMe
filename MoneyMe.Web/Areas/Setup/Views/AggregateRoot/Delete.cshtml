@model MoneyMe.Web.Models.Scaffolding.AggregateRoot

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>AggregateRoot</legend>

    <div class="display-label">Version</div>
    <div class="display-field">@Model.Version</div>

    <div class="display-label">AggregateRootEvents</div>
    <div class="display-field">@(Model.AggregateRootEvents == null ? "None" : Model.AggregateRootEvents.Count.ToString())</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}


